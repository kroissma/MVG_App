<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="at.mvgeboltskirchen.kroissma.mvgapp.client.gui.logo.LogoController">
    <top>
        <VBox spacing="5.0" BorderPane.alignment="CENTER">
            <children>
                <fx:include fx:id="tabHeader" source="../tabHeader.fxml"/>
                <Separator/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </top>
    <center>
        <ScrollPane hbarPolicy="NEVER" BorderPane.alignment="CENTER">
            <content>
                <VBox fx:id="vbLogos"/>
            </content>
        </ScrollPane>
    </center>
</BorderPane>
